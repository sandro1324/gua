{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/sounds/.*", "dest": "/sounds/$1" },
    { "src": "/cards/.*", "dest": "/cards/$1" },
    { "src": "/js/.*", "dest": "/js/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "builds": [
    { "src": "**/*", "use": "@vercel/static" }
  ]
}
